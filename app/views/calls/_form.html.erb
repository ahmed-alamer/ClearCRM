<div class="container">
	<div class="form-group">
		<label class="control-label">Employee</label>
		<input type="text" id="employee_name" class="form-control"/>
	</div>
	<div class="form-group">
		<label class="control-label">Contact</label>
		<input type="text" id="contact_name" class="form-control"/>
	</div>
	<%= f.hidden_field :employee_id %>
	<%= f.hidden_field :contact_id %>
</div>
<div class="col-md-6">
	<%= f.text_field :title %>
	<%= f.text_field :duration_hours %>
	<%= f.text_field :call_date %>
	<%= f.text_field :direction %>
</div>
<div class="col-md-6">
	<%= f.text_area :summary, :cols => 20 %>
	<%= f.text_area :notes %>
	<%= render :partial => "form_actions", :locals => {:form_layout => :basic} %>
</div>
