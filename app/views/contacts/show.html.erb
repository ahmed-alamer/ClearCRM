<div class="panel panel-info">
	  <div class="panel-heading">
			<h3 class="panel-title">Contact Information</h3>
	  </div>
	  <div class="panel-body">
	  	<div class="form-horizontal">
	  		<div class="col-md-6">
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Name</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.name %>
	  					</div>
	  				</div>
	  			</div>
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Email</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.email %>
	  					</div>
	  				</div>
	  			</div>
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Work Phone</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.work_phone %>
	  					</div>
	  				</div>
	  			</div>
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Cell Phone</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.cell_phone %>
	  					</div>
	  				</div>
	  			</div>
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Don't Call</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.do_not_call ? "Yes" : "No" %>
	  					</div>
	  				</div>
	  			</div>
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Assistant</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.assistant %>
	  					</div>
	  				</div>
	  			</div>
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Assistant Phone</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.assistant_phone %>
	  					</div>
	  				</div>
	  			</div>
	  		</div>
	  		<div class="col-md-6">
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Home Phone</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.home_phone %>
	  					</div>
	  				</div>
	  			</div>
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Fax</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.fax %>
	  					</div>
	  				</div>
	  			</div>
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Lead Source</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.lead_source %>
	  					</div>
	  				</div>
	  			</div>
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Primary Address</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.primary_address %>
	  					</div>
	  				</div>
	  			</div>
	  			<div class="form-group">
	  				<label class="col-sm-3 control-label">Alternate Address</label>
	  				<div class="col-sm-7">
	  					<div class="form-control-static">
	  						<%= @contact.alternate_address %>
	  					</div>
	  				</div>
	  			</div>
	  		</div>
	  	</div>
	  </div>
</div>